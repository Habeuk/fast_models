{% if content %}
<footer {{ attributes.addClass('footer-fnews') }}>
    <div class="container">
        <div class="fn-content">
            <div class="fn-bloc">
                {% if content %}
                <h4 {{ region_attributes.top_title.addClass('fn-title') }}>
                    {{ content.top_title }}
                </h4>
                {% endif %}
                {% if content %}
                <p {{ region_attributes.top_text_left.addClass('fn-desc') }}>
                    {{ content.top_text_left }}
                </p>
                {% endif %}
                <div class="card-social">
                    {% if content %}
                        {{ content.top_icon }}
                    {% endif %}
                    {% if content %}
                        {{ content.top_icon_one }}
                    {% endif %}
                    {% if content %}
                        {{ content.top_icon_two }}
                    {% endif %}
                </div>
            </div>
            <div class="fn-bloc">
                {% if content %}
                <h4 {{ region_attributes.top_title_one.addClass('fn-title')}}>
                    {{ content.top_title_one }}
                </h4>
                {% endif %}
                <ul class="fn-ul">
                    {% if content %}
                        {{ content.footer_top_menu }}
                    {% endif %}
                </ul>
            </div>
            <div class="fn-bloc">
                {% if content %}
                <h4 {{ region_attributes.top_title_two.addClass('fn-title')}}>
                    {{ content.top_title_two }}
                </h4>
                {% endif %}
                <ul class="fn-ul">
                    {% if content %}
                        {{ content.footer_top_menu_one }}
                    {% endif %}
                </ul>
            </div>
            <div class="fn-bloc">
                {% if content %}
                    {{ content.footer_teaser }}
                {% endif %}
            </div>

        </div>
        <div class="footer-last-bar">
            <div class="container">
                <div class="flb-content">
                    {% if content %}
                    <p {{ region_attributes.bottom_copy.addClass('explicit')}}>
                        {{ content.bottom_copy }}
                    </p>
                    {% endif %}
                    <ul class="ft-list">
                        {% if content %}
                        <li>
                            {{ content.bottom_link }}
                        </li>
                        {% endif %}
                        {% if content %}
                        <li>
                            {{ content.bottom_link_one }}
                        </li>
                        {% endif %}
                        {% if content %}
                        <li>
                            {{ content.bottom_link_two }}
                        </li>
                        {% endif %}
                    </ul>
                </div>
            </div>
        </div>
    </div>

</footer>
{% endif %}

