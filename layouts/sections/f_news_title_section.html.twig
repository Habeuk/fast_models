{% if content %}
	<section {{ attributes.addClass(['tech-stories-section-handler','container']) }}>
		{% if content.fnews_title %}
			<h3 {{ region_attributes.fnews_title.addClass('section-title','text-center') }}>
				{{ content.fnews_title }}
			</h3>
			{% if content.fnews_subtitle %}
				<h4 {{ region_attributes.fnews_subtitle.addClass('section-sub-title')}}>
					{{ content.fnews_subtitle }}
				</h4>
			{% endif %}
		{% endif %}
		<div class="row d-flex">
			{% if content.tech_content %}
				{% for i in content.tech_content|array_elements(region_attributes.tech_content.addClass('')) %}
					{{ i }}
				{% endfor %}
			{% endif %}
		</div>
		<div class="more-link">
			{% if content.more_btn_text %}
				{% for i in content.more_btn_text|array_elements(region_attributes.more_btn_text.addClass('')) %}
					{{ i }}
				{% endfor %}
			{% endif %}
		</div>


	</section>
{% endif %}
